SELECT 
 d.id,
 d.is_deleted,
 d.stage_name,
 d.mount,
 d.probability,
 d.close_date,
 d.lead_source,
 d.is_closed,
 d.is_won,
 d.forecast_category,
 d.has_opportunity_line_item,
 d.push_count,
 d.last_stage_change_date,
 d.unter_vorbehalt_c,
 d.vertrag_geschickt_c,
 d.vertrag_akzeptiert_c,
 d.widerruf_konvertiert_c,
 d.studium_beginn_datum_c,
 d.z_opp_status_c,
 d.betrag_netto_c,
 d.achtung_zweitvertrag_c,
 d.werbefreigabe_c,
 d.produkt_id_c,
 d.gesamtumsatz_c,
 d.kontakt_lead_quelle_c,
 d.kontakt_lead_quelle_detail_c,
 d.bewerberprozess_status_c,
