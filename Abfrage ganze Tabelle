FROM 
  `cognos-bildungsgruppe-ag.dlt_salesforce.contact` as a

INNER JOIN `cognos-bildungsgruppe-ag.dlt_salesforce.lead` as b
  ON a.id = b.kontakt_c

INNER JOIN `cognos-bildungsgruppe-ag.dlt_salesforce_staging.product_interest` as c
  ON b.id = c.lead_c
INNER JOIN `cognos-bildungsgruppe-ag.dlt_salesforce_staging.product_2` as d
  ON c.produkt_c = d.id
